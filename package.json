{
  "name": "vue-template",
  "description": "A vue3 project template with Vue Router, Pinia, Typescript, SCSS, Tailwindcss v3, sweetalert2 and pnpm.",
  "version": "0.0.0",
  "author": {
    "name": "Hyperbola",
    "email": "me@hyperbola.me",
    "url": "https://blog.hyperbola.me"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:wdzeng/vue3-template.git"
  },
  "private": "true",
  "packageManager": "pnpm@7.17.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint ./src --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "pinia": "^2.0.29",
    "sweetalert2": "^11.7.1",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.7",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.32.0",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "sass": "^1.57.1",
    "tailwindcss": "^3.2.4",
    "typescript": "~4.9.4",
    "vite": "^4.0.4",
    "vitest": "^0.28.2",
    "vue-tsc": "^1.0.24"
  }
}
